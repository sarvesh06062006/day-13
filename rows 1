#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int rows;
    scanf("%d",&rows);
    for(int i=1;i<=rows;i++){
        int n=(i%2==0)?0:1;
        for(int j=1;j<=rows;j++){
            printf("%d",n);
        }
        printf("\n");
    }
    return 0;
}
