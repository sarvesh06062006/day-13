#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int a = n/2-(n%2==0?1:0);
    int b = n/2;

    for (int i = 0; i < n; i++) {         
        for (int j = 0; j < n; j++) {    
            if (i >= a && i <= b || j >= a && j <=b) {
                printf("0");
            } else {
                printf("1");
            }
        }
        printf("\n");
    }

    return 0;
}
